{
    "name": "phptool/sitemap",
    "type": "project",
    "description": "sitemap framework",
    "keywords": [
        "panther"
    ],
    "license": "Apache-2.0",
    "require": {
        "php": "^7.2.5",
        "guzzlehttp/guzzle": "^6.3",
        "monolog/monolog": "^1.24",
        "samdark/sitemap": "^2.2",
        "symfony/css-selector": "^4.2",
        "symfony/dom-crawler": "^4.2",
        "symfony/panther": "^0.3.0",
        "vlucas/phpdotenv": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
    }
}
